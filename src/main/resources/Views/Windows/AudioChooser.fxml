<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="120.0" prefWidth="502.0"
  stylesheets="@../CSS/style.css" xmlns="http://javafx.com/javafx/8.0.141"
  fx:controller="User.Controllers.WindowsControllers.AudioChooserController" fx:id="root">
  <children>
    <Label alignment="CENTER" layoutX="103.0" prefHeight="49.0" prefWidth="346.0"
      text="Write your path to resources" textAlignment="CENTER" textOverrun="CLIP">
      <font>
        <Font name="Arial" size="18.0"/>
      </font>
    </Label>
    <Button defaultButton="true" layoutX="103.0" layoutY="86.0" mnemonicParsing="false"
      onAction="#writePaths" prefHeight="27.0" prefWidth="165.0" text="OK" fx:id="okBtn"/>
    <Button cancelButton="true" layoutX="284.0" layoutY="86.0" mnemonicParsing="false"
      onAction="#exit" prefHeight="27.0" prefWidth="165.0" text="Cancel"/>
    <Label layoutX="14.0" layoutY="47.0" prefHeight="27.0" prefWidth="90.0" text="AIMP player:">
      <font>
        <Font size="14.0"/>
      </font>
    </Label>
    <TextField editable="false" layoutX="103.0" layoutY="50.0" prefHeight="25.0" prefWidth="346.0"
      fx:id="audioPlayerField"/>
    <Button layoutX="458.0" layoutY="50.0" mnemonicParsing="false" onAction="#audioPlayerSelect"
      text=". . ."/>
  </children>
</AnchorPane>
